    "node-notifier": "^7.0.1",
    "ora": "^4.0.4",
    "os": "^0.1.1",
    "path-to-regexp": "^6.2.0",
    "portfinder": "^1.0.26",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "register-service-worker": "^1.7.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-copy": "^3.4.0",
