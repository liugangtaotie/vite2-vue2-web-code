    "core-js": "^3.6.4",
    "crypto-es": "^1.2.7",
    "css-loader": "^3.6.0",
    "echarts": "^4.8.0",
    "element-ui": "^2.15.0",
    "http-proxy": "^1.18.1",
    "http-server": "^0.12.3",
    "md5": "^2.2.1",
    "moment": "^2.29.1",
    "net": "^1.0.2",
    "nprogress": "^0.2.0",
